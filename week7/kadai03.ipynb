   "source": [
    "nums = [2,3,6,2,1,7,8]\n",
    "for i in range (0, len(nums)-1):\n",
    "    for j in range (0, len(nums) - i - 1):\n",
    "        if nums[j] >= nums[j+1]:\n",
    "            temp = nums[j+1]\n",
    "            nums[j+1] = nums[j]\n",
    "            nums[j] = temp\n",
    "\n",
    "print(nums)"
