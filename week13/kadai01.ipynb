       "$\\displaystyle 16 x^{4} - 48 x^{2} + 12$"
      ],
      "text/plain": [
       "16*x**4 - 48*x**2 + 12"
      ]
     },
     "execution_count": 2,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "import sympy\n",
    "\n",
    "def hermite(n,x):\n",
    "    if (n == 0):\n",
    "        return 1\n",
    "    if (n == 1):\n",
    "        return 2*x\n",
    "    return 2*x*hermite(n-1,x)-2*(n-1)*hermite(n-2,x)\n",
    "\n",
    "x = sympy.Symbol('x')\n",
    "p = hermite(4,x).simplify()\n",
    "p"
   ]
