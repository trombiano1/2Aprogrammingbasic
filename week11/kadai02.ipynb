   "source": [
    "import numpy as np\n",
    "\n",
    "acc = 10000000\n",
    "arr = (np.random.rand(3,acc) - 0.5) * 4\n",
    "plane = arr[2] > 0.5-0.5*arr[0]-0.5*arr[1]\n",
    "ellipsoidalsphere = 2 * arr[0]**2 + 3 * arr[1]**2 + arr[2]**2 < 4\n",
    "inout = plane * ellipsoidalsphere\n",
    "print(64*np.sum(inout == True)/acc)"
   ]
  }
